#!/Users/sagar.arora/Desktop/GITHUB/sunday-bhasad/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'vivd-project','console_scripts','initialize_vivd_project_db'
__requires__ = 'vivd-project'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vivd-project', 'console_scripts', 'initialize_vivd_project_db')()
    )
